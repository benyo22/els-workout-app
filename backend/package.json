    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.4",
    "argon2": "^0.41.1",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "http-status-codes": "^2.3.0",
    "mysql2": "^3.12.0",
    "nodemon": "^3.1.9",
    "object.omit": "^3.0.0",
    "sequelize": "^6.37.5"
